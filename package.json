    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "firebase": "^7.8.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.19.1",
    "tinycolor2": "^1.4.1"
  },
  "dependencies": {
    "sirv-cli": "^0.4.5",
    "swipejs": "^2.2.14"
